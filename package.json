    "nodemon": "^3.1.10",
    "prettier": "^2.8.8",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.9.2",
    "whow-project-settings": "git+https://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@github.com/whowgames/project-settings#v3.2.0"
